{
    "name": "villermen/token-bucket",
    "type": "library",
    "description": "Implementation of the Token Bucket algorithm with zend-cache support.",
    "keywords": ["token bucket", "bandwidth", "rate limit", "throttle", "throttling", "rate limiting"],
    "homepage": "https://github.com/villermen/token-bucket",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Markus Malkusch",
            "email": "markus@malkusch.de",
            "homepage": "http://markus.malkusch.de",
            "role": "Developer"
        },
        {
            "name": "Villermen",
            "email": "villermen@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"bandwidthThrottle\\tokenBucket\\": "classes/"}
    },
    "require": {
        "php": ">=5.6",
        "malkusch/lock": "^1",
        "ext-bcmath":"*"
    },
    "require-dev": {
        "ext-redis": "^2.2.4|^3",
        "predis/predis": "^1",
        "phpunit/phpunit": "^5",
        "mikey179/vfsStream": "^1.5.0",
        "php-mock/php-mock-phpunit": "^1",
        "zendframework/zend-cache": "^2.8"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        }
    }
